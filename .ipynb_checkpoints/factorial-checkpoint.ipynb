   ],
   "source": [
    "def factorial_recursive(n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    else:\n",
    "        return n * factorial_recursive(n - 1)\n",
    "\n",
    "n = 5\n",
    "result = factorial_recursive(n)\n",
    "print(f\"The factorial of {n} is {result}\")\n"
   ]
